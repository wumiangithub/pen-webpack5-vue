		"eslint-plugin-vue": "^8.6.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.8",
		"mini-css-extract-plugin": "^2.6.0",
		"node-sass": "^7.0.1",
		"ora": "^6.1.0",
		"portfinder": "^1.0.28",
